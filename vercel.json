{
    "name": "deploy-node-in-vercel",
    "version": 2,
    "public": true,
    "build": {
      "env": {
        "NODE_ENV": "production"
      },
      "command": "npm start",
      "functions": {
        "api/**/*": {
          "memory": 512,
          "runtime": "@vercel/node.16"
        }
      }
    },
    "routes": [
      {
        "src": "/.*",
        "dest": "index.js"
      }
    ]
  }
  