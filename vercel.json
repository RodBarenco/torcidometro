{
    "name": "deploy-node-in-vercel",
    "version": 2,
    "public": true,
    "build": {
      "env": {
        "NODE_ENV": "production"
      },
      "build": {
        "src": "index.js"
      } 
    },
    "routes": [
      {
        "src": "/.*",
        "dest": "index.js"
      }
    ]
  }
  